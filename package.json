{
  "name": "arma3_launcher_for_x64",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "build": "webpack -p; npm run css-build; node node_modules/cpx/bin/index.js 'bower_components/uikit/fonts/*' app/render/fonts",
    "build-dev": "webpack; npm run css-build; node node_modules/cpx/bin/index.js 'bower_components/uikit/fonts/*' app/render/fonts",
    "build-win": "webpack & npm run css-build & node node_modules/cpx/bin/index.js 'bower_components/uikit/fonts/*' app/render/fonts",
    "watch": "webpack --progress --colors --watch",
    "css-build": "node-sass --include-path scss src/sass/main.scss app/render/css/main.css",
    "app-build": "electron-packager ./app arma3launcher64 --platform=win32 --arch=x64 --version=1.6.2 --icon=app/image/main_icon.ico --overwrite",
    "app-build-unix": "electron-packager ./app arma3launcher64 --platform=darwin --arch=x64 --version=1.6.2 --icon=app/image/main_icon.ico --overwrite",
    "total-build-win": "npm run build-win & npm run app-build",
    "total-build-unix": "npm run build-dev; npm run app-build-unix"
  },
  "keywords": [
    "Arma3"
  ],
  "author": "butaosuinu",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2015-riot": "^1.1.0",
    "babel-preset-latest": "^6.24.1",
    "cpx": "^1.5.0",
    "eslint": "^3.7.1",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^2.0.1",
    "node-sass": "^4.5.0",
    "riotjs-loader": "^4.0.0",
    "sass-loader": "^6.0.3",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "jquery": "^3.1.0",
    "riot": "^3.3.2",
    "riot-router": "^0.9.7",
    "riotcontrol": "0.0.3"
  }
}
